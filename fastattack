
-- Fast Attack Script for Meme Adventure
local player = game.Players.LocalPlayer
local character = player.Character
local humanoid = character.Humanoid

local attackSpeed = 0.175 -- adjust the attack speed as needed

while true do
if humanoid:IsA("Humanoid") and humanoid:IsAttacking then
humanoid.AttackSpeed = attackSpeed
end
wait(0.1) -- adjust the loop delay as needed
end